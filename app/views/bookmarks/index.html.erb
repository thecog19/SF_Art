<%= form_for BookmarkList.new do |f| %>
  <%= f.label :name, "New List Name" %>
  <%= f.text_field :name%>
  <%= f.submit %>
<% end %>

<% @bookmark_lists.each do |list| %>
  <h4> <%= list.name %> </h4> 
  <% list.bookmarks.each do |bookmark| %>
    <%= link_to "#{bookmark.name}", location_path(bookmark.bookmarkable_id) %> <%= link_to "Delete",  bookmark_path(bookmark.id), method: :delete %>
  <% end %>
  <br>
  <br>
  <%= link_to "Delete List",  bookmark_list_path(list.id), method: :delete %>
<% end %>